{
  "name": "yomo-yama-server",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "FUJITA Setsu <fjt.seth@gmail.com>",
  "homepage": "https://github.com/sett4/yomoyama-no-yama#readme",
  "license": "MIT",
  "main": "lib/yomo-yama-server.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sett4/yomoyama-no-yama.git"
  },
  "scripts": {
    "format": "prettier --write src/**/*.{ts,tsx}",
    "prepare": "npm run gcp-build",
    "test": "jest ./src --watch",
    "posttest": "npm run lint",
    "lint": "eslint ./src/**.ts ./src/*.ts",
    "start": "node ./dist/index.js",
    "gcp-build": "tsc -p ./ --outDir ./dist",
    "deploy": "gcloud app deploy",
    "watch": "tsc -w -p ./",
    "dev": "nodemon --exec ts-node -T ./src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/sett4/yomoyama-no-yama/issues"
  },
  "devDependencies": {
    "@firebase/testing": "^0.16.14",
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.9.1",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^11.13.4",
    "@types/power-assert": "^1.5.2",
    "@types/proj4": "^2.5.0",
    "@types/sqlite3": "^3.1.6",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^24.9.0",
    "nodemon": "^1.18.11",
    "power-assert": "^1.6.1",
    "prettier": "^1.16.4",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.0.3",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "fast-csv": "^2.4.1",
    "firebase-admin": "^8.9.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.23",
    "proj4": "^2.5.0",
    "prop-types": "^15.7.2",
    "shallow-equal-object": "^1.1.1",
    "url-parse": "^1.4.4",
    "winston": "^3.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "/lib/",
      "/node_modules/"
    ],
    "extraGlobals": [
      "Uint8Array",
      "ArrayBuffer"
    ]
  }
}
