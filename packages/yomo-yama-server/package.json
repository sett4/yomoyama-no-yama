{
  "name": "yomo-yama-server",
  "version": "0.0.0",
  "description": "> TODO: description",
  "author": "FUJITA Setsu <fjt.seth@gmail.com>",
  "homepage": "https://github.com/sett4/yomoyama-no-yama#readme",
  "license": "MIT",
  "main": "lib/yomo-yama-server.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sett4/yomoyama-no-yama.git"
  },
  "scripts": {
    "format": "prettier --write src/**/*.{ts,tsx}",
    "prepare": "npm run gcp-build",
    "test": "jest ./src --watch",
    "posttest": "npm run lint",
    "lint": "eslint ./src/**.ts ./src/*.ts",
    "start": "node ./dist/index.js",
    "gcp-build": "tsc -p tsconfig-prod.json",
    "deploy": "gcloud app deploy",
    "watch": "tsc -w -p ./",
    "prefecture-wkt-csv": "ts-node -T ./src/cli/prefecture-wkt-csv.ts",
    "dev": "NODE_ENV=development nodemon --exec ts-node -T ./src/index.ts"
  },
  "bugs": {
    "url": "https://github.com/sett4/yomoyama-no-yama/issues"
  },
  "engines": {
    "node": "14.x.x"
  },
  "devDependencies": {
    "@firebase/rules-unit-testing": "^1.2.11",
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "@types/bunyan": "^1.8.6",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.2.1",
    "nodemon": "^1.18.11",
    "power-assert": "^1.6.1",
    "prettier": "^1.16.4",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.0.3"
  },
  "dependencies": {
    "@fast-csv/format": "^4.3.5",
    "@fast-csv/parse": "^4.3.6",
    "@google-cloud/logging-bunyan": "^3.0.2",
    "@types/cheerio": "^0.22.18",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.9.1",
    "@types/jsonstream": "^0.8.30",
    "@types/minimist": "^1.2.0",
    "@types/moment-timezone": "^0.5.12",
    "@types/node": "^11.13.4",
    "@types/power-assert": "^1.5.2",
    "@types/proj4": "^2.5.0",
    "@types/sqlite3": "^3.1.6",
    "@types/url-parse": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "axios": "^0.21.1",
    "bunyan": "^1.8.15",
    "cheerio": "^1.0.0-rc.3",
    "express": "^4.17.1",
    "firebase-admin": "^9.7.0",
    "JSONStream": "^1.3.5",
    "moment": "^2.29.2",
    "moment-timezone": "^0.5.23",
    "proj4": "^2.5.0",
    "prop-types": "^15.7.2",
    "shallow-equal-object": "^1.1.1",
    "typescript": "^3.9.7",
    "url-parse": "^1.4.4"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfig": "tsconfig.json"
      }
    },
    "testPathIgnorePatterns": [
      "/lib/",
      "/node_modules/"
    ],
    "extraGlobals": [
      "Uint8Array",
      "ArrayBuffer"
    ]
  },
  "nodemonConfig": {
    "env": {
      "NODE_ENV": "development"
    },
    "verbose": true
  }
}
